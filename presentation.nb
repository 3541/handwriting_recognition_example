Cell[CellGroupData[{

Cell["A Simple Network", "Section",
 CellChangeTimes->{{3.732529525829897*^9, 
  3.7325295437262506`*^9}},ExpressionUUID->"6df000ef-91be-4518-ae17-\
40436d2a2b21"],

Cell["\<\
A first attempt at a letter classifier.

Network design, even for experts is largely based on heuristics and not \
objective principles.
Let\[CloseCurlyQuote]s try something very simple:\
\>", "Text",
 CellChangeTimes->{{3.7325295513750677`*^9, 
  3.732529600726698*^9}},ExpressionUUID->"6dc1cf18-f93b-4aa1-a935-\
bf2daa7aba29"],

Cell[BoxData[
 RowBox[{"simpleUntrained", "=", 
  RowBox[{"NetInitialize", "[", "\[IndentingNewLine]", 
   RowBox[{"NetChain", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"LinearLayer", "[", "108", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "LogisticSigmoid", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"LinearLayer", "[", "36", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "LogisticSigmoid", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"SoftmaxLayer", "[", "]"}]}], "\[IndentingNewLine]", "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"\"\<Input\>\"", "\[Rule]", 
      RowBox[{"NetEncoder", "[", 
       RowBox[{"{", 
        RowBox[{"\"\<Image\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"28", ",", "28"}], "}"}], ",", 
         RowBox[{"\"\<ColorSpace\>\"", "\[Rule]", "\"\<Grayscale\>\""}]}], 
        "}"}], "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"\"\<Output\>\"", "\[Rule]", 
      RowBox[{"NetDecoder", "[", 
       RowBox[{"{", 
        RowBox[{"\"\<Class\>\"", ",", "charset"}], "}"}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->
  "In[140]:=",ExpressionUUID->"e84285c4-60e3-44c4-b8d5-19b7865386e6"],

Cell[BoxData[
 RowBox[{
  RowBox[{"simpleUntrained", "=", 
   RowBox[{"Import", "[", 
    RowBox[{
     RowBox[{"NotebookDirectory", "[", "]"}], "<>", 
     "\"\<simpleTrainedFinal.wlnet\>\""}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.782948559302898*^9, 3.782948591819709*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"6d977266-fc71-4960-9370-00583408b55d"],

Cell[TextData[{
 "LinearLayer just calculates ",
 StyleBox["w . x + b.\n", "Code"],
 "ElementWiseLayer applies the given function.\nSoftmaxLayer normalizes a \
vector so its components sum to 1.\nTraining is easy:"
}], "Text",
 CellChangeTimes->{{3.732529648588795*^9, 3.7325296963778977`*^9}, {
  3.7325297413576465`*^9, 3.732529798361342*^9}, {3.7325300657981997`*^9, 
  3.732530068774784*^9}, {3.732530202340208*^9, 
  3.7325302320544987`*^9}},ExpressionUUID->"6e29c1f3-ecff-4f1c-8cb8-\
530905045bd6"],

Cell[BoxData[
 RowBox[{
  RowBox[{"NetTrain", "[", 
   RowBox[{"simpleUntrained", ",", "trainingData", ",", "All", ",", 
    RowBox[{"ValidationSet", "\[Rule]", "validationData"}], ",", 
    RowBox[{"TargetDevice", "\[Rule]", "\"\<GPU\>\""}]}], "]"}], "[", 
  "\"\<TrainedNet\>\"", "]"}]], "Input",
 CellChangeTimes->{{3.7325297805280895`*^9, 3.7325297811696916`*^9}, 
   3.7325300773610287`*^9},ExpressionUUID->"0833950a-4955-477c-9043-\
a185cdb89591"]
}, Open  ]]
}, Open  ]],

