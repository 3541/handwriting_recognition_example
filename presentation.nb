Cell["Convolutional Neural Networks", "Section",
 CellChangeTimes->{{3.732530239709668*^9, 
  3.7325302572204757`*^9}},ExpressionUUID->"d552862e-e600-4c4e-9ee0-\
6594aa317d95"],

Cell[TextData[{
 "Make use of convolutional layers, designed to emulate how real neurons \
respond to visual input.\n\nEach neuron has a ",
 StyleBox["receptive field",
  FontSlant->"Italic"],
 ".\n\nAs well as pooling layers, which combine output from a cluster of \
neurons into one."
}], "Text",
 CellChangeTimes->{{3.7325302608637857`*^9, 
  3.7325303913571877`*^9}},ExpressionUUID->"7be7dd43-c353-49bc-8050-\
b788559b3ea6"],

Cell[BoxData[
 RowBox[{"convUntrained", "=", 
  RowBox[{"NetInitialize", "[", "\[IndentingNewLine]", 
   RowBox[{"NetChain", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ConvolutionLayer", "[", 
        RowBox[{"20", ",", "4"}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "Ramp", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"PoolingLayer", "[", 
        RowBox[{"2", ",", "2"}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ConvolutionLayer", "[", 
        RowBox[{"40", ",", "9"}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "Ramp", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"PoolingLayer", "[", 
        RowBox[{"3", ",", "3"}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"LinearLayer", "[", "500", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "Ramp", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"LinearLayer", "[", "36", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"SoftmaxLayer", "[", "]"}]}], "\[IndentingNewLine]", "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"\"\<Input\>\"", "\[Rule]", 
      RowBox[{"NetEncoder", "[", 
       RowBox[{"{", 
        RowBox[{"\"\<Image\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"28", ",", "28"}], "}"}], ",", 
         RowBox[{"\"\<ColorSpace\>\"", "\[Rule]", "\"\<Grayscale\>\""}]}], 
        "}"}], "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"\"\<Output\>\"", "\[Rule]", 
      RowBox[{"NetDecoder", "[", 
       RowBox[{"{", 
        RowBox[{"\"\<Class\>\"", ",", "charset"}], "}"}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}], "]"}]}]], "Input",
