Cell["Training Data", "Section",
 CellChangeTimes->{{3.732529869034214*^9, 
  3.732529876792611*^9}},ExpressionUUID->"18e50e92-d541-4f18-9423-\
543cfc71e7f8"],

Cell["\<\
Before we construct a network, we need data with which to teach it.
I used the NIST SD 19 handwritten digit database.
NetTrain expects data in the form of a list of associations between input and \
output.\
\>", "Text",
 CellChangeTimes->{{3.732529879668043*^9, 3.7325299085823298`*^9}, {
  3.732529997091032*^9, 
  3.732530009951621*^9}},ExpressionUUID->"3c68ac95-ab9d-4d42-af8d-\
20768717aadb"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"charset", "=", 
   RowBox[{"Join", "[", 
    RowBox[{
     RowBox[{"ToUpperCase", "[", 
      RowBox[{"Alphabet", "[", "]"}], "]"}], ",", 
     RowBox[{"Range", "[", 
      RowBox[{"0", ",", "9"}], "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"workDir", "=", 
   RowBox[{"NotebookDirectory", "[", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"importDirectory", "[", "d_", "]"}], ":=", 
  RowBox[{"With", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"dir", "=", 
      RowBox[{"workDir", "<>", "\"\<data/org/\>\"", "<>", 
       RowBox[{"ToString", "[", "d", "]"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"ParallelMap", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"File", "[", "#", "]"}], "\[Rule]", "d"}], "&"}], ",", 
      RowBox[{"FileNames", "[", 
       RowBox[{"\"\<*.png\>\"", ",", "dir"}], "]"}]}], "]"}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
 CellLabel->
  "In[141]:=",ExpressionUUID->"3816437c-9b8c-4819-8bfa-f72cbd92df2a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"dataFiles", "=", 
   RowBox[{"RandomSample", "[", 
    RowBox[{
     RowBox[{"importDirectory", "/@", "charset"}], "//", "Flatten"}], "]"}]}],
   ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"trainingData", "=", 
   RowBox[{
    RowBox[{"Partition", "[", 
     RowBox[{"dataFiles", ",", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Length", "[", "dataFiles", "]"}], "-", "1"}], ")"}], "/", 
        "2"}], "-", "1"}]}], "]"}], "\[LeftDoubleBracket]", "1", 
    "\[RightDoubleBracket]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"validationData", "=", 
   RowBox[{
    RowBox[{"Partition", "[", 
     RowBox[{"dataFiles", ",", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Length", "[", "dataFiles", "]"}], "-", "1"}], ")"}], "/", 
        "2"}], "-", "1"}]}], "]"}], "\[LeftDoubleBracket]", "2", 
    "\[RightDoubleBracket]"}]}], ";"}]}], "Input",ExpressionUUID->"4b15f55d-\
6e6d-4f7c-b57c-e64d0b1f94d9"],

